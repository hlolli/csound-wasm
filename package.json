    "build:node": "yarn prebuild && NODE_ENV=production TARGET=node webpack --mode production"
  },
  "devDependencies": {
    "arraybuffer-loader": "^1.0.8",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
