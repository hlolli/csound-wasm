  "main": "./dist/libcsound.js",
  "keywords": [
    "csound",
    "csound-wasm",
    "wasm",
    "webassembly",
    "audio",
    "webaudio",
    "wasi"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "nix-shell nix/compile.nix",
    "compile:debug": "nix-shell nix/compile.nix --arg debug true",
    "compile:dev": "nix-shell nix/compile.nix --arg debug true --arg dev true",
    "lint": "eslint 'src/**/*.js'",
    "watch": "webpack --watch --mode develop",
    "dev": "webpack-dev-server",
    "prebuild": "node script/worklet-concat-files.js",
    "prepublish": "yarn prebuild && NODE_ENV=production webpack --mode production",
