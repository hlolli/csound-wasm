    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-closure": "^0.1.1",
    "html-webpack-plugin": "^4.3.0",
    "raw-loader": "^4.0.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "workerize-loader": "1.1.0"
